{
  "name": "engineeringpark",
  "version": "0.0.0",
  "description": "Decentraland Engineering Park",
  "scripts": {
    "start": "dcl preview --no-browser",
    "build": "metaverse-compiler build.json",
    "coverage": "npm test -- --coverage",
    "watch": "metaverse-compiler build.json --watch",
    "test": "jest --watch"
  },
  "author": "Tristan Martindale",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "ajv": "^6.5.2",
    "jest": "^23.4.2",
    "metaverse-api": "^4.0.4",
    "ts-jest": "^23.1.3",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "redux": "^4.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/App.tsx",
      "<rootDir>/src/utils.ts",
      "<rootDir>/src/*/**.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "verbose": true,
    "testURL": "http://localhost/"
  }
}
